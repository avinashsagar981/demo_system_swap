import time

start_time = time.time()  # Record the starting time

# ... Your code that you want to time goes here ...
# For example:
result = 1
for i in range(100000000):
    result *= i  # Some calculation

end_time = time.time()  # Record the ending time

elapsed_time = end_time - start_time  # Calculate the elapsed time

print(f"Time taken to execute: {elapsed_time:.6f} seconds") # Print the time taken
